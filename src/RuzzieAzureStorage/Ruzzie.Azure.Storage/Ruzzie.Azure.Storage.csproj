<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>8.0</LangVersion>
    <NullableContextOptions>enable</NullableContextOptions>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>1.0.0.0</Version>
    <Authors>Dorus Verhoeckx</Authors>
    <Company>D Crafting</Company>
    <Copyright>Copyright © Dorus Verhoeckx 2019</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/Ruzzie/Ruzzie.Azure.Storage</RepositoryUrl>
    <PackageProjectUrl>https://github.com/Ruzzie/Ruzzie.Azure.Storage</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>Ruzzie.Azure.Storage.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup>
    <NoWarn>1701;1702;1591</NoWarn>
    <Description>.net helpers and utilities for Azure storage</Description>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ruzzie.Common" Version="2.0.54" />
    <PackageReference Include="WindowsAzure.Storage" Version="9.3.3" />
  </ItemGroup>
</Project>